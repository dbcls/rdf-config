## cheminf:CHEMINF_000562 (International nonproprietary name, INN)
## used for obtaining WHO ATC code
- INN :
  - a: cheminf:CHEMINF_000562
  - sio:is-attribute-of:
    - compound_with_inn: compound:CID1983
  - dcterms:subject*:
    - atc_code: ATC
    - mesh_concept_with_inn: mesh:M0000115
  - sio:has-value+:
    - international_nonproprietary_name: "paracetamol"

# ATC code
- ATC concept:ATC_N02BE01:
  - a: skos:concept
  - skos:prefLabel:
    - atc_preflabel: '"paracetamol"@en'
  - skos:broader?:
    - parent_atc: concept:ATC_N02BE
  - pav:importedFrom:
    - atc_source: source:WHO
  - skos:inScheme:
    - atc_scheme: concept:ATC
