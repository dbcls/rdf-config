# 4.1 PubChem Compound


- Compound compound:CID2519:
#  - a:  #different from molecule to molecule
#    - obo:CHEBI_27732
  - obo:has_role*:
    - role: vocab:FDAApprovedDrugs
  - vocab:is_active_ingredient_of*:
    - product: ndfrt:N0000145919
  - cheminf:CHEMINF_000461*: # is stereoisomer of (object example is from L-Glutamic Acid)
    - stereoisomer: compound:CID57470133
  - cheminf:CHEMINF_000455*: # is isotopologue of
      - isotopomer: compound:CID64136 #14C caffeine
  - vocab:has_parent?: #object example is from caffeine monohydrate
    - parent_compound: compound:CID2519  #caffeine
  - cheminf:CHEMINF_000478*: # has_component
    - component_compound: compound:CID962 #water
  - cheminf:CHEMINF_000462*: # has same connectivity as (object example is from L-Glutamic Acid)
    - compound_with_same_connectivity: compound:CID450555

  # Attributes : See # 4.3 Descriptor
  - sio:has-attribute+:
    - compound_identifier: CompoundIdentifier
    - preferred_iupac_name: PreferredIUPACName
    - molecular_formula: MolecularFormula
    - molecular_weight: MolecularWeight
    - mono_isotopic_weight: MonoIsotopicWeight
    - exact_mass: ExactMass
    - iupac_inchi: IUPACInChI
    - isomeric_smiles: IsomericSMILES
    - canonical_smiles: CanonicalSMILES
    - non_hydrogen_atom_count: NonHydrogenAtomCount
    - isotope_atom_count: IsotopeAtomCount
    - defined_atom_stereo_count: DefinedAtomStereoCount
    - undefined_atom_stereo_count: UndefinedAtomStereoCount
    - defined_bond_stereo_count: DefinedBondStereoCount
    - undefined_bond_stereo_count: UndefinedBondStereoCount
    - hydrogen_bond_acceptor_count: HydrogenBondAcceptorCount
    - hydrogen_bond_donor_count: HydrogenBondDonorCount
    - rotatable_bond_count: RotatableBondCount
    - covalent_unit_count: CovalentUnitCount
    - structure_complexity: StructureComplexity
    - xlogp3: XLogP3
    - tpsa: TPSA
    - total_formal_charge: TotalFormalCharge

  - skos:closeMatch*:
    - wikidata: wikidata:Q60235

